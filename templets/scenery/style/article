layui.use(["flow","jquery"],function(){var t=layui.flow,n=layui.jquery;article.Init(n);t.load({elem:"#LAY_bloglist",done:function(t,i){var u=n(".bloglist").attr("data-pagecount"),e=n(".bloglist").attr("data-count"),f=n(".bloglist").attr("data-type"),r=[];console.log(t);n.ajax({type:"POST",url:"/Blog/PageLoadBlogByType",data:{TypeID:f,page:t},success:function(f){f==""?n(".bloglist .layui-flow-more").css("display","none"):(r.push(f),i(r.join(""),t<u))}})}})})